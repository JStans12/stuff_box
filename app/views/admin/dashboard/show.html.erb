<h1>Admin Dashboard Page</h1>
<div class="container">
  <% @users.each do |user| %>
    <div class="user">
      <p>
        Username: <%= user.username %>
      </p>
      <p>
        Email: <%= user.email %>
      </p>
      <p>
        Cellphone: <%= user.cellphone %>
      </p>
      <p>
        User status: <%= user.status %>
      </p>
      <p>
        Root Folder #: <%= user.root %>
      </p>
    </div>
  <% end %>
</div>
